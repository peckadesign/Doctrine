{
	"name": "kdyby/doctrine",
	"type": "library",
	"description": "Doctrine integration into Nette Framework",
	"keywords": ["nette", "kdyby", "doctrine", "orm", "dbal"],
	"homepage": "http://kdyby.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Filip Proch√°zka",
			"homepage": "http://filip-prochazka.com",
			"email": "filip@prochazka.su"
		}
	],
	"support": {
		"email": "filip@prochazka.su",
		"issues": "https://github.com/kdyby/doctrine/issues"
	},
	"require": {
		"php": "^7.4",
		"doctrine/orm": "~2.6.6",
		"doctrine/dbal": "~2.10.4",
		"kdyby/events": "dev-master || ^3.1.1",
		"kdyby/console": "dev-master || ~2.7.1",
		"kdyby/annotations": "dev-master as 2.5.0",
		"kdyby/doctrine-cache": "dev-master as 2.6.2",
		"kdyby/strict-objects": "^1.0 || ^2.0",
		"nette/di": "^2.4 || ^3.0",
		"nette/utils": "^2.5 || ^3.0",
		"nette/finder": "^2.5",
		"tracy/tracy": "2.6.7"
	},
	"require-dev": {
		"nette/bootstrap": "^2.4 || ^3.0",
		"nette/caching": "^2.5.3 || ^3.0",
		"nette/http": "^2.4.7 || ^3.0",
		"nette/tester": "^2.0"
	},
	"autoload": {
		"psr-0": {
			"Kdyby\\Doctrine\\": "src/",
			"Kdyby\\Persistence\\": "src/"
		},
		"classmap": [
			"src/Kdyby/Doctrine/exceptions.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/KdybyTests"
		]
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/Jakub-Fajkus/DoctrineCache"
		},
		{
			"type": "vcs",
			"url": "https://github.com/Jakub-Fajkus/Annotations"
		}
	]
}
